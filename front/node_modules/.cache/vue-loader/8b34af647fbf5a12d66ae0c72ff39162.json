ews/Signin.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"signin\">\n    <h2 class=\"title\">Connection :</h2>\n\n    <form v-if=\"local||session\"> \n      <p>vous etes deja connecter !</p>\n      <div>\n        <p>se deconnecter ?</p>\n        <button type=\"button\" @click=\"signout\">deconnecté</button>\n      </div>\n    </form>\n\n    <form v-else>\n      <input type=\"email\" v-model=\"email\" placeholder=\"email\"/>\n      <input type=\"password\" v-model=\"password\" placeholder=\"password\"/>\n      <div>\n        <label for=\"remember\">Rester connecté</label>\n        <input type=\"checkbox\" id=\"remember\" v-model=\"remember\"/>\n        <button type=\"button\" @click=\"signin\">connection</button>\n      </div>\n    </form>\n\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n  name: 'Signin',\n  data () {\n    return {\n     email: null,\n     password: null,\n     remember: false,\n     api: null,\n     local: false,\n     session: false\n\n    }\n  },\n  mounted () {\n    this.check();\n  },\n\n  methods: {\n\n    signout() {\n      localStorage.removeItem('userId');\n      localStorage.removeItem('token');\n      this.local = false;\n\n      sessionStorage.removeItem('userId');\n      sessionStorage.removeItem('token');\n      this.session = false;\n\n      this.check();\n    },\n\n    check() {\n      if (sessionStorage.getItem('userId')) {\n        this.session = true;\n      } else if (localStorage.getItem('userId')) {\n        this.local = true;\n      } else {\n        return;\n      }\n    },\n\n    storage(bool) {\n      if (!bool) {\n        sessionStorage.setItem('userId', this.api.userId) ;\n        sessionStorage.setItem('token', this.api.token );\n        this.session = true;\n      } else {\n        localStorage.setItem('userId', this.api.userId) ;\n        localStorage.setItem('token', this.api.token );\n        this.session = true;\n      }\n    },\n\n    signin() {\n      axios.post('http://localhost:3000/api/users/signin', {\n        email: this.email,\n        password: this.password\n      })\n      .then((res) => {\n          this.api = {\n            userId: res.data.userId,\n            token: res.data.token };\n          this.storage(this.remember)\n        })\n      .catch(error => console.log(error));   \n    }\n  }\n}\n\n</script>\n\n<style lang=\"scss\" scoped>\nh2 {\n  font-weight: normal;\n}\nform {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  input {\n    margin: .5em;\n    padding: .3em 1em;\n        width: 15em;\n        border-radius: 4px;\n        border: 1px solid darkgray;\n        &#remember {\n            width: auto;\n            vertical-align: middle;\n        }\n        &:focus {\n            outline: none;\n            box-shadow: 0px 0px 5px darkgrey;\n        }\n  }\n}\n</style>"]}]}