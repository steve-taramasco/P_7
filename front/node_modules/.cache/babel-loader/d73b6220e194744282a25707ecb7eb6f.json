,CAAL;AAAA,OARX;AASF;AAZO;AATI,CAAf","sourcesContent":["<template>\n  <div class=\"signin\">\n    <h2 class=\"title\">Connection :</h2>\n\n    <form >\n      <input type=\"email\" v-model=\"email\" placeholder=\"email\"/>\n      <input type=\"password\" v-model=\"password\" placeholder=\"password\"/>\n      <div>\n        <label for=\"remember\">Rester connect√©</label>\n        <input type=\"checkbox\" id=\"remember\"/>\n        <button type=\"button\" @click=\"login\">connection</button>\n      </div>\n    </form>\n\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'Signin',\n  data () {\n    return {\n      email: null,\n      password: null,\n    }\n  },\n\n  methods: {\n\n    login() {\n      axios.post('http://localhost:3000/api/users/signin', {\n        email: this.email,\n        password: this.password\n      })\n      .then((res) => {\n        console.log(res)\n        this.$router.push(\"/users/\" + res.data.userId);\n      })\n      .catch(error => console.log(error));   \n    }\n  }\n}\n\n</script>\n\n<style lang=\"scss\" scoped>\nh2 {\n  font-weight: normal;\n}\nform {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  input {\n    margin: .5em;\n    padding: .3em 1em;\n        width: 15em;\n        border-radius: 4px;\n        border: 1px solid darkgray;\n        &#remember {\n            width: auto;\n            vertical-align: middle;\n        }\n        &:focus {\n            outline: none;\n            box-shadow: 0px 0px 5px darkgrey;\n        }\n  }\n}\n</style>"],"sourceRoot":""}]}